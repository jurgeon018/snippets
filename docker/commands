docker run nginx
docker run -d --name nginx -p 8080:80 nginx
docker exec -ti nginx bash
docker rm -f nginx
docker run -d --name nginx -p 80:80 -v /static/content:/usr/share/nginx/html nginx
docker run -it --rm python:3.4






docker rm -f $(docker ps -a -q) # kill all containers



